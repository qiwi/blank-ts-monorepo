{
  "name": "blank-ts-monorepo-infra",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Infra",
  "keywords": [],
  "main": "target/es5/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es5/index.d.ts",
  "typescript": {
    "definition": "target/es5/index.d.ts"
  },
  "files": [],
  "scripts": {
    "postupdate": "yarn",
    "infra:npm-run-all": "cd $INIT_CWD && npm-run-all",
    "infra:libdefkit": "cd $INIT_CWD && libdefkit",
    "infra:buildstamp": "cd $INIT_CWD && buildstamp",
    "infra:c8": "cd $INIT_CWD && c8",
    "infra:depcheck": "cd $INIT_CWD && depcheck --ignores='typedoc,tslib,earljs,uvu,eslint-*,prettier-*,**/*-infra' --ignore-patterns='typings'",
    "infra:eslint": "cd $INIT_CWD && eslint",
    "infra:prettier": "cd $INIT_CWD && prettier",
    "infra:rimraf": "cd $INIT_CWD && rimraf",
    "infra:terser": "cd $INIT_CWD && terser",
    "infra:mkdirp": "cd $INIT_CWD && mkdirp",
    "infra:tsc": "cd $INIT_CWD && tsc",
    "infra:uglify": "./scripts/sh/uglify.sh"
  },
  "devDependencies": {
    "@qiwi/libdefkit": "^4.0.8",
    "@qiwi/npm-run-all": "^4.1.7",
    "buildstamp": "^2.0.0",
    "c8": "^7.12.0",
    "cpy-cli": "^4.1.0",
    "depcheck": "^1.4.3",
    "earljs": "^0.2.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-qiwi": "^1.17.4",
    "mkdirp": "^1.0.4",
    "prettier": "^2.7.1",
    "prettier-config-qiwi": "^1.7.2",
    "rimraf": "^3.0.2",
    "terser": "^5.14.2",
    "typescript": "^4.7.4"
  },
